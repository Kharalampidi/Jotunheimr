{
  "name": "jotunheimr",
  "version": "1.5.1",
  "description": "Microservice for processing images and uploading them to a designated bucket on AWS S3",
  "main": "lib/index.js",
  "directories": {
    "test": "test",
    "src": "src",
    "lib": "lib"
  },
  "scripts": {
    "start": "supervisor -w src -- src/server.litcoffee",
    "build": "coffee --compile --bare --output lib/ src/",
    "prepublish": "coffee --bare --compile --output lib/ src/",
    "postpublish": "rm -rf lib/*",
    "test": "NODE_ENV=test mocha -w -b -c --check-leaks test/index.coffee -R progress --compilers coffee:coffee-script/register,litcoffee:coffee-script/register",
    "test-drone": "NODE_ENV=test node_modules/.bin/mocha -b -c --check-leaks test/index.coffee -R spec --compilers coffee:coffee-script/register,litcoffee:coffee-script/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Turistforeningen/Jotunheimr.git"
  },
  "keywords": [
    "Express",
    "Connect",
    "Upload",
    "AWS",
    "S3",
    "Images"
  ],
  "author": "Hans Kristian Flaatten <hans.kristian.flaatten@turistforeningen.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turistforeningen/Jotunheimr/issues"
  },
  "homepage": "https://github.com/Turistforeningen/Jotunheimr",
  "dependencies": {
    "async": "~0.9",
    "coffee-script": "~1",
    "compression": "~1.4",
    "dms2dec": "~1.0",
    "express": "~4",
    "librato": "~0.1",
    "morgan": "~1",
    "multer": "~0.1",
    "raven": "~0.7",
    "response-time": "~2",
    "s3-uploader": "~0.8"
  },
  "devDependencies": {
    "mocha": "~2",
    "supertest": "~0.15",
    "supervisor": "~0.6"
  }
}

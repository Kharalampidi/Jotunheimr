{
  "name": "jotunheimr",
  "version": "1.9.1",
  "description": "Microservice for processing images and uploading them to a designated bucket on AWS S3",
  "main": "lib/index.js",
  "directories": {
    "test": "test",
    "src": "src",
    "lib": "lib"
  },
  "scripts": {
    "start": "TMPDIR=/tmp supervisor src/server.litcoffee",
    "build": "coffee --bare --compile --output lib/ src/",
    "test": "NODE_ENV=test TMPDIR=/tmp mocha -b -c --check-leaks test/index.coffee -R spec --compilers coffee:coffee-script/register,litcoffee:coffee-script/register",
    "watch": "NODE_ENV=test TMPDIR=/tmp mocha -w -b -c --check-leaks test/index.coffee -R progress --compilers coffee:coffee-script/register,litcoffee:coffee-script/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Turistforeningen/Jotunheimr.git"
  },
  "keywords": [
    "Express",
    "Connect",
    "Upload",
    "AWS",
    "S3",
    "Images"
  ],
  "author": "Hans Kristian Flaatten <hans.kristian.flaatten@dnt.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turistforeningen/Jotunheimr/issues"
  },
  "homepage": "https://github.com/Turistforeningen/Jotunheimr",
  "dependencies": {
    "coffee-script": "^1.10.0",
    "compression": "^1.6.0",
    "dms2dec": "^1.1.0",
    "express": "^4.13.3",
    "librato": "^0.1.0",
    "morgan": "^1.6.0",
    "multer": "^1.1.0",
    "newrelic": "^1.24.0",
    "raven": "^0.9.0",
    "response-time": "^2.3.0",
    "s3-uploader": "^1.1.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "mocha": "^2.3.4",
    "supertest": "^1.1.0",
    "supervisor": "^0.9.1"
  }
}

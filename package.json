{
  "name": "jotunheimr",
  "version": "1.7.0",
  "description": "Microservice for processing images and uploading them to a designated bucket on AWS S3",
  "main": "lib/index.js",
  "directories": {
    "test": "test",
    "src": "src",
    "lib": "lib"
  },
  "scripts": {
    "start": "TMPDIR=/tmp ./node_modules/.bin/supervisor lib/server.js",
    "dev": "TMPDIR=/tmp ./node_modules/.bin/supervisor src/server.litcoffee",
    "build": "./node_modules/.bin/coffee --bare --compile --output lib/ src/",
    "test": "NODE_ENV=test TMPDIR=/tmp node_modules/.bin/mocha -b -c --check-leaks test/index.coffee -R spec --compilers coffee:coffee-script/register,litcoffee:coffee-script/register",
    "watch": "NODE_ENV=test TMPDIR=/tmp mocha -w -b -c --check-leaks test/index.coffee -R progress --compilers coffee:coffee-script/register,litcoffee:coffee-script/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Turistforeningen/Jotunheimr.git"
  },
  "keywords": [
    "Express",
    "Connect",
    "Upload",
    "AWS",
    "S3",
    "Images"
  ],
  "author": "Hans Kristian Flaatten <hans.kristian.flaatten@turistforeningen.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turistforeningen/Jotunheimr/issues"
  },
  "homepage": "https://github.com/Turistforeningen/Jotunheimr",
  "dependencies": {
    "async": "~1.2",
    "coffee-script": "~1",
    "compression": "~1.5",
    "dms2dec": "~1.0",
    "express": "~4",
    "librato": "~0.1",
    "morgan": "~1",
    "multer": "~0.1",
    "raven": "~0.7",
    "response-time": "~2",
    "s3-uploader": "1.0.0-rc.4",
    "supervisor": "~0.6"
  },
  "devDependencies": {
    "mocha": "~2",
    "supertest": "~1"
  }
}
